

// Callsite 
// r3 := Func(11)
val 11 --> r1 

val 0 --> r4
add r6 r4 --> r2 // r6--> r2, rip is before current instruction  $$$, rIP

jmp Func1

// Func sets return value in r3

add r3 r3 --> r4

// r3 should be 22
// r4 should be 44 
exit


// Function  Body . x --> x*2
// arg1 is in r1
// return address in in r2
Func1:

add r1 r1 --> r3

// Return 
val 2 --> r1  // convention, 2 instructions later 
add r2 r1 --> r2
jmp r2




